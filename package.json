{
  "name": "ringcentral-notification-integration-helper",
  "version": "1.37.3",
  "description": "RingCentral Engage Voice API js wrapper",
  "main": "dist/engage-voice",
  "module": "src/engage-voice.ts",
  "types": "dist/engage-voice",
  "scripts": {
    "test": "standard && jest --coverage",
    "release": "rm -rf dist && webpack --progress --config webpack.config.babel.js",
    "prepublishOnly": "npm run release",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/engage-voice-js.git"
  },
  "keywords": [
    "RingCentral",
    "notification",
    "integration",
    "API",
    "js",
    "javascript",
    "wrapper",
    "SDK"
  ],
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringcentral/engage-voice-js/issues"
  },
  "homepage": "https://github.com/ringcentral/engage-voice-js#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@tsconfig/recommended": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "dotenv-override-true": "^6.2.2",
    "jest": "^26.6.3",
    "standard": "^16.0.3",
    "ts-loader": "^8.0.11",
    "ts-standard": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "./test"
    ],
    "setupFiles": [
      "dotenv-override-true/config"
    ],
    "silent": false
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "/dist/"
    ],
    "globals": [
      "MessageEvent"
    ]
  },
  "directories": {
    "test": "test"
  }
}
